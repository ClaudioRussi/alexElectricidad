<header id="nav-blue-menu" class="row">
  <div id="menu-icons" class="row col-s-10  menu-icons">
    <div class="col-s-11">
      <%=link_to image_tag("logoAlexAzul.png", {alt: "Logo", class: 'burger-menu-logo'}), root_path%>
    </div>
    <div class="col-s-1">
      <a href="javascript:void(0)" onclick="openNav()" id="blue-openbtn"><%=image_tag "menu-azul.png", alt:"Cerrar menú", class:"social-icon"%></a>
    </div>
  </div>
  <div id="my-sidenav" class="row sidenav">
    <div class="row menu-icons">
      <div class="row col-s-10 ">
        <div class="col-s-11">
          <%=link_to image_tag('logoAlex.png', {alt:'Logo de alex', class: 'burger-menu-logo'}), :root%>
        </div>
        <div class="col-s-1">
          <a href="javascript:void(0)" class="closebtn" onclick="closeNav();"><%=image_tag "cancel-cross.png", alt:"Cerrar menú", class:"social-icon"%></a>
        </div>
      </div>
    </div>

    <div id="nav-links" class="row">
      <div class="col-s-10">
        <%=link_to "INICIO", root_path%>
        <%=link_to "SERVICIOS", services_path%>
        <%=link_to "SOBRE NOSOTROS", about_path%>
        <%=link_to "NOTICIAS", publications_path%>
        <%=link_to "CONTACTO", contact_path%>
      </div>
    </div>
  <!--  <div class="row">
      <div class="">
        <a href="facebook.com"><%=image_tag 'facebook-logo.png', alt:'Enlace a Facebook', class:'social-icon'%></a>
      </div>
    </div>-->
    <div class="row social-media-menu">
      <div class=" col-s-10">
        <div class="col-s-11">
          <ul>
            <li>
              <%= link_to image_tag('facebook-logo.png',{alt:'Enlace a Facebook', class:'social-icon'}), "http://www.facebook.com"%>
            </li>
            <li>
              <%= link_to image_tag('twitter-logo.png',{alt:'Enlace a Twitter', class:'social-icon'}), "http://www.twitter.com"%>
            </li>
            <li>
              <%= link_to image_tag('instagram-logo.png',{alt:'Enlace a Instagram', class:'social-icon'}), "http://www.instagram.com"%>
            </li>
          </ul>
        </div>
      </div>
    </div>

  </div>
</header>
